<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_cons"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E0DCA7"
    tools:context="com.example.LifeCo.activities.SplashScreenNew"
    tools:deviceIds="wear">

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/lifeco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/obatsplash1"
        android:layout_width="197dp"
        android:layout_height="130dp"
        android:alpha=".2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/obat2" />

    <ImageView
        android:id="@+id/obatsplash2"
        android:layout_width="197dp"
        android:layout_height="130dp"
        android:alpha=".2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/obat2" />
</androidx.constraintlayout.widget.ConstraintLayout>